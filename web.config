<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="Python FastCGI" 
     path="*" 
     verb="*" 
     modules="FastCgiModule" 
     scriptProcessor="C:\inetpub\wwwroot\IClassSel_WebService\.venv\Scripts\python.exe|C:\inetpub\wwwroot\IClassSel_WebService\.venv\Lib\site-packages\wfastcgi.py" 
     resourceType="Unspecified" 
     requireAccess="Script" />
    </handlers>
  </system.webServer>
  <appSettings>
    <add key="PYTHONPATH" value="C:\inetpub\wwwroot\IClassSel_WebService" />
    <add key="WSGI_HANDLER" value="app.app" />
    <add key="WSGI_LOG" value="C:\inetpub\wwwroot\IClassSel_WebService\logs\wfastcgi.log" />
  </appSettings>
</configuration>